{
  "tests": [
    {
      "name": "Login with valid credentials",
      "method": "POST",
      "body": {
        "email": "admin@estate.com",
        "password": "admin"
      },
      "expectedStatus": 200,
      "expectedBody": {
        "success": true,
        "user": {
          "email": "admin@estate.com",
          "role": "admin"
        }
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Login with invalid credentials",
      "method": "POST",
      "body": {
        "email": "wrong@email.com",
        "password": "wrong"
      },
      "expectedStatus": 401,
      "expectedBody": {
        "error": "string"
      },
      "bodyMatcher": "partial"
    }
  ]
}
